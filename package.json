{
  "name": "my-app1",
  "type": "module",
  "scripts": {
    "dev": "bun run scripts/dev.ts",
    "start:hot": "bun run --hot src/index.ts",
    "start": "bun run src/index.ts",
    "compile": "bun run tsc -p src/client/ --outDir static/",
    "tailwind": "bun run tailwindcss --input src/styles/main.css --output static/main.css",
    "migrate:up": "bun run src/db/migrator.ts --up",
    "migrate:d": "bun run src/db/migrator.ts --down",
    "seed": "bun run src/db/seed.ts",
    "test": "bun test --watch"
  },
  "dependencies": {
    "@conform-to/zod": "^1.8.2",
    "@node-rs/argon2": "^2.0.2",
    "@oslojs/crypto": "^1.0.1",
    "@oslojs/encoding": "^1.1.0",
    "@tailwindcss/cli": "^4.1.11",
    "@types/jest": "^29.5.14",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.13",
    "es-toolkit": "^1.39.7",
    "hono": "^4.8.5",
    "kysely": "^0.27.6",
    "kysely-bun-sqlite": "^0.3.2",
    "neverthrow": "^8.2.0",
    "tailwindcss": "^4.1.11",
    "ts-jest": "^29.4.0",
    "zod": "^4.0.5"
  },
  "devDependencies": {
    "@types/alpinejs": "^3.13.11",
    "@types/bun": "latest",
    "daisyui": "^5.0.46",
    "jest": "^29.7.0",
    "typescript": "^5.8.3"
  },
  "trustedDependencies": [
    "@parcel/watcher",
    "bcrypt"
  ]
}
